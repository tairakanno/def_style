<div class="chat-header">
</div>
<div class="chat-main">
</div>
<%= form_with model: @message class: 'form', local: true do |f| %> 
  <div class="form-input">
     <%= f.text_field :text, class:  'form-message', placeholder: 'type a message' %>
  </div>
  <%= f.submit '送信', class: 'form-submit' %>
<% end %>